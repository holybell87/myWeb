    var url = '/zf_user/popup/areaPrint/printId/resume_print_area';
	window.open(url, 'printWin', 'width=750, height=600, scrollbars=yes');
}

/**
 * 인재디비 사용 레이어
 */
function useConfirmMandb(el, confirmLayerId) {
    var url = '/zf_user/jobs/talent/use-confirm';
    var res_idx = $('data_form').res_idx.value;
    
	new Ajax.Request(url, {
	    parameters: {res_idx : res_idx},
	    onSuccess: function (tr) {
	    	if (tr.responseJSON) {
	            var result = tr.responseJSON;
	            var layerId = '';

	            if ( result.userGroup == 'c' && result.mandb_search_cnt > 0 ) {
	            	//$('mandb_num').innerHTML = '<b>'+ (result.mandb_search_cnt-1) + '건</b>';

	            	layerId = confirmLayerId;
	            } else {
		            layerId = 'service-guide-layer';
	            }

	            if ( layerId ) {
    	            elPos = $(el).positionedOffset();
                	
                	$(layerId).style.top  = elPos.top - 100;
                    $(layerId).style.left = 200;
                    
                	$(layerId).show();
	            }
	        }
	    },
	    onComplete: function (tr) {
	    	switch ( tr.status ) {
	        case 200 :
                break;
	        case 203 :
                // 203 Non-Authoritative Information
                alert('로그아웃 되었습니다. 다시 로그인해 주십시오.');
                location.reload();
                break;
	        case 500 :
                alert('서버에서 오류가 발생했습니다. 잠시후 다시 시도해 주십시오.');
                break;
	        default :
                if(confirm('페이지에 오류가 발생했습니다. 페이지를 새로 고치려면 확인을 눌러 주십시오.')) {
                    document.location.reload();
                }
                break;
            }
        }
    });
}

/**
 * 스크랩
 */
var ingScrap = false;
function scrap() {
    var complete_mode = '';
    if ( $('data_form').mem_gb.value == 'c' ) {
        if ( ingScrap ) {
            return;
        }

        ingScrap = true;
        var url = '/zf_user/company/mandb-scrap-ajax';
        var res_idx = $('data_form').res_idx.value;
        new Ajax.Request(url, {     
            parameters: {res_idx : res_idx, type : 'button'},
            onSuccess: function (tr) {
                if ( tr.responseJSON ) {
                    var result = tr.responseJSON;

                    if ( result.success == true ) {
                        if (pandora == 'y') {
                            if(!!window.opener && complete_mode!='y') {
                                if ( !!window.opener.document.getElementById('side_hidden_scrap_talent_count') ) {
                                    var hiddenCnt = opener.document.getElementById('side_hidden_scrap_talent_count').innerHTML;
                                    var scrapCnt = parseInt(hiddenCnt);
                                    scrapCnt++;
                                    // 우측 사이드 메뉴 count 처리
                                    opener.document.getElementById('side_hidden_scrap_talent_count').innerHTML = scrapCnt;
                                    if (scrapCnt > 99) {
                                        opener.document.getElementById('side_scrap_talent_count').innerHTML = '99+';
                                    } else {
                                        opener.document.getElementById('side_scrap_talent_count').innerHTML = scrapCnt;
                                    }
                                }
                            }
                        } else {
                            if (document.getElementById('side_hidden_scrap_talent_count')) {
                                var hiddenCnt = document.getElementById('side_hidden_scrap_talent_count').innerHTML;
                                var scrapCnt = parseInt(hiddenCnt);

                                scrapCnt++;
                                // 우측 사이드 메뉴 count 처리
                                sidewidgetcount(scrapCnt);
                            }
                        }
                    	try { n_trackEvent('mandb_scrap', 'scrap_complete', res_idx, 'view'); } catch(e) {}
                        alert(result.msg);
                    } else {
                	    alert(result.msg);
                    }
                }
                ingScrap = false;
            }
            ,onException : function(e) {
                alert('통신의 문제가 발생하였습니다.');
                ingScrap = false;
            }
        });
    } else {
        var pageUrl = '';
        if ( $('data_form').page_url.value ) {
            pageUrl = $('data_form').page_url.value;
        }
        location.href = '/zf_user/login/index/page_url/' + pageUrl;
    }
}

function sidewidgetcount(scrapCnt) {
    $('side_hidden_scrap_talent_count').update(scrapCnt);

    if(scrapCnt > 99) {
        $('side_scrap_talent_count').update('99+');
    } else {
        $('side_scrap_talent_count').update(scrapCnt);
    }
}

//SMS 발송 체크
function checkSendingSMS(mem_idx) {
    new Ajax.Request('/zf_user/memcom/sms/check-sending-sms-validation-ajax', {
        parameters: {
            type : 'mandb',
            idx : mem_idx
        },
        asynchronous: false,
        onSuccess: function(transport) {
            var json = transport.responseText.evalJSON();
            
            if ( json.result == 'empty' ) {
                alert('문자발송이 불가능한 번호입니다.');
                return;
            }

            sendSMS(json.mem_idx);            
        },

        onFailure: function() {
            alert('서버의 응답이 없습니다.\n다시 시도하여 주시기 바랍니다.');
            return false;
        }
    });
}

//SMS 발송
function sendSMS(mem_idx) {
    $('smsForm').mode.value = 'group';
    $('smsForm').mem_idx.value = mem_idx;
    $('smsForm').type.value = 'mandb';

    var targetWin = 'send_sms';
    win_sms = window.open('about:blank', targetWin, 'width=527,height=670,scrollbars=no,status=no') ;

    $('smsForm').target = targetWin;
    $('smsForm').submit();
    win_sms.focus();
}

// 이메일 발송
function sendMail(mem_idx, res_idx) {
    window.open('/zf_user/company/recruit-apply-sendmail?mode=person&mem_idx='+mem_idx+'&res_idx='+res_idx, 'sendmail', 'width=760, height=710, top=200, left=300, status=yes, resizable=no, scrollbars=yes').focus();
}
function sendProtectMail(mem_idx, res_idx, sKey) {
    var url = '/zf_user/company/recruit-apply-sendmail?mode=person&level=protect&mem_idx='+mem_idx+'&res_idx='+res_idx,
        sKey = sKey || '';

    if ( sKey ) {
        url += '&sKey=' + sKey;
    }
    
    window.open(url, 'sendmail', 'width=760, height=710, top=200, left=300, status=yes, resizable=no, scrollbars=yes').focus();
}

//휴대폰 번호 유효성 검사
function checkCellNumber(cell) {
    var reg_pattern = /([^0-9]|^)01(0|1|6|7|8|9)\s*((\&gt;){0,2}|[\+\-\)\.\/\~\>\_]{0,2})\s*[1-9][0-9]{2,3}\s*((\&gt;){0,2}|[\+\-\)\.\/\~\>\_]{0,2})\s*[0-9]{4}/m;
    
    return reg_pattern.test(cell);
}

