// onload event
Event.observe(window, 'load', function () {
//    document.getElementById('edit_resume_layer').style.display = 'none';
    
    // 인쇄 모드일 경우
    if ( $('data_form') ) {
        var mode = $('data_form').mode.value;
        
        if ( mode == 'print' ) {
            printResume();        
        } else if ( mode == 'mobile' ) {
            hideHeader();
            hideDisablePrintArea();
        } else if (mode == 'academy') {
            hideHeader();
        }else {
            initHeader();
        }
    }

	if ( $('attach_port_files_view') ) {
    
		$$('.portfolio_list_more img').invoke('observe','click',function(){
			if ( $('more_btn').hasClassName('more')) {
				$('more_btn').src = "http://www.saraminimage.co.kr/person/new/btn_portfolio_closed.png";
				$('more_btn').addClassName('close');
