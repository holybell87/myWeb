/**
 * 수정 레이어 열기
 */
function showEditResumeLayer(el) {
    if ( $('template_cd').value == 2 ) {
        goEditingResume();
        return;
    }
    
    var pos = $(el).positionedOffset();
    var topPos  = pos.top + 11;
    var leftPos = pos.left - 26;
    $('edit_resume_layer').setStyle({ top : topPos + 'px', left : leftPos + 'px' });
    $('edit_resume_layer').show();

    setTimeout(function() {
        $('resume_print_area').onclick = function () {
            hideEditResumeLayer();
        };
    }, 1000);
}

/**
 * 수정 레이어 닫기
 */
function hideEditResumeLayer() {
    $('edit_resume_layer').hide();
    $('resume_print_area').onclick = '';

    $('resume_print_area').stopObserving('click');
}

/**
 * 이력서 수정 페이지로 이동
 */
function goEditingResume() {
    if ( opener && !opener.closed ) {
        opener.location.href = '/zf_user/member/resume-manage/write?res_idx=' + $('data_form').res_idx.value;
        self.close();
    }
}

/**
 * 자기소개서 수정 페이지로 이동
 */
function goEditingIntroduce() {
    if ( opener && !opener.closed ) {
        parent.opener.location.href = '/zf_user/resume/write-resume-contents?write_mode=modify&res_idx=' + $('data_form').res_idx.value;
        self.close();
    }
}

/**
 * 인쇄
 */
function printResume() {
    if ( $('data_form').controller.value == 'mandb' || $('data_form').event_resume_auth ) {
        printArea();
    } else {
        hideDisablePrintArea();
        window.print();    
        initHeader();
        showDisablePrintArea();
    }
}

/**
 * 영역 인쇄
 */
